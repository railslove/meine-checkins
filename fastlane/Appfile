require 'json'

appJSON = JSON.parse(File.read("./app.json"))
appIdentifier = appJSON['id']

# Android
package_name(appIdentifier)
json_key_file("~/service-account-key.json") # Path to the json secret file - Follow https://docs.fastlane.tools/actions/supply/#setup to get one

# iOS
app_identifier(appIdentifier)
